    <div class="card">
      <div class="card-header card-no-border">
        <div class="header-top"> 
          <h5>Check Room Availability</h5>
          <div class="dropdown icon-dropdown">
            <button class="btn dropdown-toggle" id="activitydropdown" type="button" data-bs-toggle="dropdown" aria-expanded="false"><i class="icon-more-alt"></i></button>
            <div class="dropdown-menu dropdown-menu-end" aria-labelledby="activitydropdown"><a class="dropdown-item" href="#">Today</a><a class="dropdown-item" href="#">Tomorrow</a><a class="dropdown-item" href="#">Yesterday </a></div>
          </div>
        </div>
      </div>
      <div class="card-body pt-0">
        <div class="calendar-container">
          <div class="calendar-header">
            <button class="prev-month" (click)="changeMonth(-1)">&#10094;</button>
            <div class="month-year">
              <span>{{ monthName }}</span>
              <span>{{ currentYear }}</span>
            </div>
            <button class="next-month" (click)="changeMonth(1)">&#10095;</button>
          </div>
          <table class="calendar">
            <thead>
              <tr>
                <th>Sun</th>
                <th>Mon</th>
                <th>Tue</th>
                <th>Wed</th>
                <th>Thu</th>
                <th>Fri</th>
                <th>Sat</th>
              </tr>
            </thead>
            <tbody>
              <tr *ngFor="let week of calendarDays">
                <td *ngFor="let day of week" [class.active]="isToday(day)" (click)="openRoomAvailability(day,'toOpenCalendar')">
                  {{ day.date }}
                </td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
